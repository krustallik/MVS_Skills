@model UserInfo
@{
    ViewData["Title"] = "My Hello Page";
}

<div class="">
    <h1 class="display-4">Hello @Model.Name</h1>


    <table class="table table-hover">
        <tbody>
            @foreach (var skill in Model.Skills)
            {
                <tr>
                    <td>@skill.Title</td>
                </tr>
            }
        </tbody>
    </table>
</div>
